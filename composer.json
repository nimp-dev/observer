{
    "name": "nimp/observer",
    "description": "minimal realization lets the text editor object notify other service objects about changes in its state",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nimp\\Observer\\": "src/"
        }
    },
    "minimum-stability": "RC",
    "require": {
      "php": ">=8.3",
      "psr/event-dispatcher": "1.*"
    },
    "require-dev": {
      "phpunit/phpunit": "^11.2",
        "phpstan/phpstan": "^2.1"
    },
    "autoload-dev": {
      "psr-4": {
        "Nimp\\Observer\\Tests\\": "tests/"
      }
    },
    "scripts": {
      "test": "phpunit --colors=always",
      "phpstan": "vendor/bin/phpstan analyse src --level=6"
    }

}
